	"version": "1.0.0",
	"description": "con-prisma-react-graphql-tutorial-back",
	"main": "index.js",
	"prisma": {
		"seed": "ts-node-dev prisma/seed.ts"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"start": "node dist/index",
		"build": "npm run clean && npm i && npm run generate && npx tsc",
