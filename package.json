	"devDependencies": {
		"@types/express": "^4.17.14",
		"@types/node": "^18.11.11",
		"prisma": "^4.7.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@pothos/core": "^3.23.1",
